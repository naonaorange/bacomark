{
  "expo": {
    "name": "bacomark",
    "slug": "bacomark",
    "version": "4.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "android": {
      "permissions": ["CAMERA"],
      "package": "com.naonaorange.bacomark",
      "versionCode": 2
    },
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "CFBundleLocalizations" : ["ja"],
        "CFBundleDevelopmentRegion" : "ja_JP",
        "NSCameraUsageDescription": "This app uses the camera to scan barcodes on products."
      },
      "bundleIdentifier": "com.naonaorange.bacomark",
      "buildNumber": "4.0.0"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "apiKey": ""
    },
    "description": ""
  }
}
